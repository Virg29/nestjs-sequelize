		"dotenv": "^16.4.4",
		"jest": "29.5.0",
		"jest-mock-extended": "^3.0.5",
		"pg": "^8.11.3",
		"pg-hstore": "^2.3.4",
		"reflect-metadata": "^0.1.13",
		"sequelize": "^6.37.0",
		"sequelize-typescript": "^2.1.6",
		"uuid": "^9.0.0"
	},
